<!doctype html>
<html lang="en">
  <head>

    {{ wp_head }}
    
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="{{ site.description }}" />
    <meta name="author" content="" />
    <link rel="icon" href="{{ site.theme.link }}/favicon.ico" />
    <title>{{ site.title }}</title>
    <link href="{{ site.theme.link }}/assets/dist/app.css" rel="stylesheet" />
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png?v=wAdrE5xLpY">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png?v=wAdrE5xLpY">
    <link rel="icon" type="image/png" sizes="192x192" href="/android-chrome-192x192.png?v=wAdrE5xLpY">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png?v=wAdrE5xLpY">
    <link rel="manifest" href="/site.webmanifest?v=wAdrE5xLpY">
    <link rel="mask-icon" href="/safari-pinned-tab.svg?v=wAdrE5xLpY" color="#003c5e">
    <link rel="shortcut icon" href="/favicon.ico?v=wAdrE5xLpY">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="/mstile-144x144.png?v=wAdrE5xLpY">
    <meta name="theme-color" content="#ffffff">
  </head>

  <body class="{{body_class}}" data-template="base.twig">

    {% include 'partials/header.twig' %}

    <main role="main">
      <section class="widthFull container">

        <div class="header section-inner widthGlobal">

          <div class="page-controls">

            {% if post.next %}
                <a href="{{ post.next.link }}" style="flex: 1;">Prev<h5 class="title">{{ post.next.title }}</h5></a>
            {% else %}
                <div> </div>
            {% endif %}

            {% include 'partials/work-home.twig' %}

            {% if post.prev %}
                <a href="{{ post.prev.link }}" style="text-align: right;flex: 1;">Next<h5 class="title">{{ post.prev.title }}</h5></a>
            {% else %}
                <div> </div>
            {% endif %}

          </div>

          <div class="page-title hero-title">
            <h1>{{ post.title }}</h1>

                {% if projectdate %}
              <h3 class="date">{{ projectdate }}</h3>
            {% endif %}

          </div>



        </div>

        </div>

      </section>

      {% block content %}{% endblock %}

      <section class="widthFull container">

        <div class="section-inner widthGlobal">

          
          {# include 'partials/pagination.twig' #}

          <div class="page-controls">

            {% if post.next %}
                <a href="{{ post.next.link }}">Prev<h5 class="title">{{ post.next.title }}</h5></a>
            {% endif %}

            {% include 'partials/work-home.twig' %}

            {% if post.prev %}
                <a href="{{ post.prev.link }}" style="text-align: right">Next<h5 class="title">{{ post.prev.title }}</h5></a>
            {% endif %}

          </div>


        </div>

      </section>

    </main>

    {% include 'partials/footer.twig' %}

    {{ wp_footer }}

    <script src="{{ site.theme.link }}/assets/dist/app.js"></script>

  </body>

</html>